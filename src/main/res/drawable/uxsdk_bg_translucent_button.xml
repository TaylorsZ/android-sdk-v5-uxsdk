<?xml version="1.0" encoding="utf-8"?>
<!-- 半透明、圆角、带边框的按钮背景 -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 填充颜色 -->
    <solid android:color="@color/uxsdk_transparent" />

    <!-- 边框颜色和宽度 -->
    <stroke
        android:width="1dp"
        android:color="@color/uxsdk_orange_in_dark" />

    <!-- 圆角半径 -->
    <corners android:radius="5dp" />
</shape>